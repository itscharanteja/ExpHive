{
  "expo": {
    "name": "ExpHive",
    "slug": "expense-hive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/expense.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "owner": "itscharanteja",
    "plugins": ["expo-router", "expo-notifications", "expo-font"],
    "splash": {
      "image": "./assets/images/expense.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false,
      "barStyle": "dark-content"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itscharanteja.expensehive"
    },
    "android": {
      "package": "com.itscharanteja.exphive",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/expense.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "88a39d41-96fe-42f6-9aea-7c8b46745864"
      }
    },

    "newArchEnabled": true
  }
}
